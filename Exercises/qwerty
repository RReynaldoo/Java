package qwerty;
import java.util.Scanner;

public class NumbersDemo {
	public static void main(String[] j) {
		Scanner input = new Scanner(System.in);
		
		System.out.println();
		System.out.println();
	}
	
	public static void displayTwiceTheNumber(double x) {
		
		double result = x * 2;
	}
	
	public static void displayNumberPlusFive(double x) {
		double result = x + 5;
	}
	
	public static void displayNumberSquared(double x) {
		double result = x * x;
	}
	
}